{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --watch src/server src/server",
    "dev:webpack": "webpack-dev-server --mode development --devtool inline-source-map --hot",
